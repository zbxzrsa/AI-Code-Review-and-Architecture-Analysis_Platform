# Production Environment Variables
# Disable mock API in production
VITE_USE_MOCK_API=false

# API Base URL
VITE_API_BASE_URL=https://api.coderev.example.com

# Auth Service URL
VITE_AUTH_SERVICE_URL=https://auth.coderev.example.com

# WebSocket URL
VITE_WS_URL=wss://api.coderev.example.com

# App Configuration
VITE_APP_NAME=AI Code Review Platform
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DEBUG_LOGGING=false
